{
  "name": "experimental",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "emotion",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "debug": "NODE_OPTIONS=\"--inspect\" next dev -H 0.0.0.0",
    "start": "next start",
    "db:reset": "node ./scripts/db_reset.js",
    "db:seed": "node ./scripts/db_seed.js",
    "db:prepare": "yarn run db:reset && yarn run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "cy": "cypress open",
    "compress": "node ./scripts/compress_images.js"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
