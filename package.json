    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-feather": "^2.0.8",
    "react-postprocessing": "^1.4.0",
    "react-query": "^2.23.1",
    "react-responsive": "^8.1.0",
    "react-spring": "^8.0.27",
    "react-three-fiber": "^5.0.3",
    "sass": "^1.27.0",
    "tailwindcss": "^1.9.5",
    "three": "^0.121.1",
