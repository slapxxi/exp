    "babel-jest": "^26.3.0",
    "cypress": "^5.3.0",
    "date-fns": "^2.16.1",
    "drei": "^1.5.7",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "jotai": "^0.10.0",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.2",
    "next": "^9.5.5",
    "postcss-preset-env": "^6.7.0",
